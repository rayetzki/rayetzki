{
  "name": "rayetzki",
  "version": "0.0.1",
  "description": "Portfolio of Illia Raietskyi",
  "source": "src/index.html",
  "browserslist": [
    "defaults"
  ],
  "scripts": {
    "clean": "rimraf dist .cache",
    "dev": "yarn clean && yarn parcel:serve",
    "build": "yarn clean && yarn parcel:build",
    "parcel:serve": "parcel src/*.html -p 5000",
    "parcel:watch": "parcel watch src/*.html --public-url='./'",
    "parcel:build": "parcel build src/*.html --public-url='./'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rayetzki/rayetzki.git"
  },
  "keywords": [
    "Portfolio",
    "Web Dev",
    "Dev Portfolio"
  ],
  "author": "Illia Raietskyi @rayetzki",
  "license": "ISC",
  "homepage": "https://github.com/rayetzki/rayetzki#readme",
  "devDependencies": {
    "parcel": "latest",
    "posthtml-expressions": "^1.9.0",
    "posthtml-include": "^1.7.2",
    "posthtml-load-config": "^2.0.0",
    "rimraf": "^3.0.2"
  }
}
